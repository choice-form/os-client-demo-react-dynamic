{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "no-console": false,
    "max-classes-per-file": false,
    "member-ordering": [
      true,
      {
        "order": [
          "public-static-field",
          "private-instance-field",
          "public-instance-field",
          "public-constructor",
          "private-instance-method",
          "protected-instance-method",
          "public-instance-method"
        ]
      }
    ],
    "variable-name": false
  }
}